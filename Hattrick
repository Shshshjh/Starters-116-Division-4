#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin>>t;
    
    while(t--)
    {
        int count=0;
        int flag=0;
        
        for(int i=0;i<6;i++)
        {
            string s;
            cin>>s;
            if(s=="W")
            {
                count++;
            }
            else
            {
                count=0;
            }
            if(count==3)
            {
                flag=1;
                
            }
            
        }
        if(flag==1)
        cout<<"yes"<<endl;
        else
        cout<<"no"<<endl;
    }
}
